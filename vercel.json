{
  "version": 2,
  "functions": {
    "api/discord.js": { "runtime": "nodejs20.x" }
  },
  "routes": [
    { "src": "/api/discord", "methods": ["POST", "OPTIONS"], "dest": "/api/discord" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "methods": ["GET", "HEAD"], "dest": "/index.html" }
  ]
}