{
  "version": 2,
  "functions": {
    "api/discord.js": { "runtime": "nodejs20.x" }
  },
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "testes.js", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "teste.css", "use": "@vercel/static" },
    { "src": "images/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/discord", "methods": ["POST", "OPTIONS"], "dest": "/api/discord" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "methods": ["GET", "HEAD"], "dest": "/index.html" }
  ]
}